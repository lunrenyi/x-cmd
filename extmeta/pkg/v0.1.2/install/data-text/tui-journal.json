{
  "lang": "Rust",
  "homepage": "https://github.com/AmmarAbouZor/tui-journal",
  "desc": {
    "cn": "一个在终端中书写、管理和组织笔记的工具。",
    "en": "A tool for writing, managing and organizing your notes in the terminal."
  },
  "rule": {
    "arch/pacman": {
      "cmd": "sudo pacman -S tui-journal",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#arch-linux"
    },
    "alpine/apk": {
      "cmd": "apk add tui-journal",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#alpine-linux"
    },
    "freebsd/pkg": {
      "cmd": "pkg install -y tui-journal",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#freebsd"
    },
    "netbsd/pkgin": {
      "cmd": "pkgin install tui-journal",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#netbsd"
    },
    "nix/nix-env": {
      "cmd": "nix-env -iA nixos.tui-journal",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#nix"
    },
    "darwin/brew": {
      "cmd": "brew install AmmarAbouZor/homebrew-tui-journal/tui-journal",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#homebrew-macos-and-linux"
    },
    "linux/brew": {
      "cmd": "brew install AmmarAbouZor/homebrew-tui-journal/tui-journal",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#homebrew-macos-and-linux"
    },
    "debian/apt": {
      "cmd": "sudo apt install -y libssl-dev",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#linux-dependencies"
    },
    "ubuntu/apt": {
      "cmd": "sudo apt install -y libssl-dev",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#linux-dependencies"
    },
    "kali/apt": {
      "cmd": "sudo apt install -y libssl-dev",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#linux-dependencies"
    },
    "raspbian/apt": {
      "cmd": "sudo apt install -y libssl-dev",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#linux-dependencies"
    },
    "mint/apt": {
      "cmd": "sudo apt install -y libssl-dev",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#linux-dependencies"
    },
    "fedora/dnf": {
      "cmd": "sudo dnf install openssl-devel",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#linux-dependencies"
    },
    "/cargo": {
      "cmd": "cargo install tui-journal --locked",
      "reference": "https://github.com/AmmarAbouZor/tui-journal?tab=readme-ov-file#install-with-default-features"
    }
  }
}
