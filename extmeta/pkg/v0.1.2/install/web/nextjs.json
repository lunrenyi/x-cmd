{
  "lang": "JavaScript",
  "homepage": "https://nextjs.org/",
  "desc": {
    "cn": "Next.js 是一个用于构建全栈 Web 应用程序的 React 框架。",
    "en": "Next.js is a React framework for building full-stack web applications."
  },
  "rule": {
    "/npx": {
      "cmd": "npx create-next-app@latest",
      "reference": "https://nextjs.org/docs/app/getting-started/installation#automatic-installation"
    },
    "/npm": {
      "cmd": "npm install next@latest react@latest react-dom@latest",
      "reference": "https://nextjs.org/docs/app/getting-started/installation#manual-installation"
    }
  }
}
