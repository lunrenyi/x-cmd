# shellcheck shell=dash

___x_cmd_pacman___xproxy(){
    [ $# -gt 0 ]    ||  set - ls
    local op="$1";      shift

    local O=pacman
    case "$op" in
        ls|use|unuse|try|untry|run)
            ___x_cmd proxy wrap "$op"   "$@" ;;
        -h|--help)
            ___x_cmd help -m pacman proxy      "$@" ;;
    esac
}
