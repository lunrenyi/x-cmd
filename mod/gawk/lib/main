# shellcheck shell=dash

xrc:mod:lib     gawk   __runcmd

___x_cmd_gawk___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd_gawk___help            "$@" ;;
        --|--runmain)   ___x_cmd_gawk___runmain         "$@" ;;
        *)              ___x_cmd_gawk___runmain "$op"   "$@" ;;
    esac
}

___x_cmd_gawk___help(){
    # ___x_cmd help -m gawk   "$@"
    ___x_cmd_gawk___runmain --help
}
