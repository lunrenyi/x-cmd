# shellcheck shell=dash

xrc:mod:lib     solo    engine/run  engine/runmain

___x_cmd_solo_engine(){
    [ $# -gt 0 ]  ||  set -- --help

    local op="$1";  shift
    case "$op" in
        run|run_main)
                ___x_cmd_solo_engine_"$op"   "$@" ;;

        *)      N=engine M="Unsupported subcmd -> $op" log:ret:64
    esac
}
