# shellcheck    shell=dash

___x_cmd_jsr___runmain(){
    ___x_cmd_jsr___runcmd "$@"
}

___x_cmd_jsr___runcmd(){
    if ___x_cmd_hascmd jsr; then
        ___x_cmd_jsr___runcmd(){  ___x_cmd_cmds jsr "$@"; }
    else
        ___x_cmd_jsr___install || return
        ___x_cmd_jsr___runcmd(){ "$___X_CMD_jsr_BIN_PATH/$___X_CMD_jsr_BIN" "$@" ; }
    fi || return
    ___x_cmd_jsr___runcmd "$@"
}
