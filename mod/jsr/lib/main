# shellcheck shell=dash

xrc:mod:lib     jsr     add ls

# x jsr x-cmd:plwr page
# x jsr plwr page
# x jsr plwr browser

# x jsr puppeteer:abc work

___x_cmd_jsr___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1";      shift
    case "$op" in
        -h|--help)      ___x_cmd help -m jsr    "$@";    return 0 ;;
        add|ls)
                        ___x_cmd_jsr_"$op"      "$@" ;;
        *)              N=jsr M="Unknown op -> $op" log:ret:64 ;;
    esac
}

