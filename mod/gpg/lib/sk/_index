
xrc:mod:lib     gpg     sk/ls  sk/id

___x_cmd_gpg_sk(){
    [ $# -gt 0 ]    ||      set -- ls

    local op="$1";  shift

    case "$op" in
        -h|--help)          ___x_cmd help -m gpg sk    "$@"     ;;

        ls|id|edit|export|send|receive|search|refresh|import)
                            ___x_cmd_gpg_sk_"$op"       "$@"    ;;
        rm|delete)          ___x_cmd_gpg_sk_rm          "$@"    ;;
    esac
}




