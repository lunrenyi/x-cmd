
___x_cmd_icmd___runmain(){
    ___x_cmd_icmd___runcmd "$@"
}

___x_cmd_icmd___runcmd(){
    if ___x_cmd_hascmd icmd; then
        ___x_cmd_icmd___runcmd(){  ___x_cmd_cmds icmd "$@"; }
    else
        ___x_cmd_icmd___install || return
        ___x_cmd_icmd___runcmd(){ "$___X_CMD_icmd_BIN_PATH/$___X_CMD_icmd_BIN" "$@" ; }
    fi || return
    ___x_cmd_icmd___runcmd "$@"
}
