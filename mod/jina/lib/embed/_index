# shellcheck shell=dash disable=SC2034

xrc:mod:lib  jina  \
    embed/generate embed/send embed/model/_index    \
    embed/worker

___x_cmd_jina_embed(){
    [ $# -gt 0 ]    ||      set -- --help
    local op="$1";  shift
    case "$op" in
        model|generate|worker)
                            ___x_cmd_jina_embed_"$op"       "$@" ;;
        -h|--help)          ___x_cmd help -m jina embed     "$@" ;;
    esac
}


