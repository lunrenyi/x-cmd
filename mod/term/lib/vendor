# shellcheck shell=dash

___x_cmd_term_vendor(){
    [ $# -gt 0 ] || {
        local x_=
        ___x_cmd_term_vendor_
        printf "%s\n" "$x_"
        return
    }

    case "$1" in
        --unset)
            ___X_CMD_TERM_VENDOR=""
            ;;
        *)
            ___X_CMD_TERM_VENDOR="$1"
            export ___X_CMD_TERM_VENDOR
            x:info "setting term vendor as $1"
            return
            ;;
    esac
}

___x_cmd_term_vendor_(){
    ___x_cmd __termvendor_
}
