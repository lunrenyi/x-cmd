___x_cmd_ensurefp(){
    if [ "$#" -eq 0 ]; then
        local srcfile; while read -r srcfile; do
            case "$srcfile" in
                */*)        [ -d "${srcfile%/*}" ] || ___x_cmd_cmds_mkdir -p "${srcfile%/*}" || return 1
            esac
        done
        return
    else
        while [ $# -gt 0 ]; do
            case "$1" in
                */*)        [ -d "${1%/*}" ] || ___x_cmd_cmds_mkdir -p "${1%/*}" || return 1
            esac
            shift
        done
    fi
}
