
___x_cmd_json2yml(){
    case "$#" in
        0)  ___x_cmd_main yq e -P ;;
        1)  ___x_cmd_main yq e -P <"$1" ;;
        2)  ___x_cmd_main yq e -P <"$1" >"$2" ;;
        *)  x:error "Accept at most two arguments."; return 1 ;;
        # *)  ___x_cmd_main yq -o json e -P <"$1" >"$2" ;;
    esac
}
