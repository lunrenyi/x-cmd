
if [ -n "$ZSH_VERSION" ]; then
    . "$___X_CMD_ROOT_MOD/x-cmd/lib/lwmod/__pathuniq1"
else

case "$BASH_VERSION" in
    3.*|"")
___x_cmd___pathuniq(){
    local IFS=:
    local newpath=:
    local p; for p in $PATH; do
        case "$newpath" in
            *":${p}:"*) ;;
            *)      newpath="${newpath}${p}:"
        esac
    done
    PATH="${newpath#:}"
    PATH="${PATH%:}"
}   ;;

    *)  . "$___X_CMD_ROOT_MOD/x-cmd/lib/lwmod/__pathuniq_bash" ;;
esac

fi
