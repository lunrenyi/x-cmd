# shellcheck shell=dash

___x_cmd log init moon

xrc:mod:lib     moon        __runcmd    __install

___x_cmd_moon___main(){
    [ "$#" -gt 0 ] ||   set -- --help

    local op="$1"; shift

    case "$op" in
        --help|-h)      ___x_cmd_moon___help "$@"; return ;;
        --install)      ___x_cmd_moon___install ;;
        *)              ___x_cmd_moon___runmain "$op" "$@" ;;
    esac
}

___x_cmd_moon___help(){
    ___x_cmd help -m moon "$@"
}

