
___x_cmd_xrc_reset(){
    ___x_cmd_unexport ___X_CMD_ROOT_MOD
    if [ -n "$SHELL" ]; then                            exec "$SHELL"
    elif [ -f "/bin/$___X_CMD_SHELL" ]; then            exec "/bin/$___X_CMD_SHELL"
    elif [ -f "/usr/bin/$___X_CMD_SHELL" ]; then        exec "/usr/bin/$___X_CMD_SHELL"
    fi
}
