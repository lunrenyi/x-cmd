
___x_cmd_xrc_cat___(){
    while [ $# -gt 0 ]; do
        ___x_cmd_cmds_cat "$___X_CMD_ROOT_MOD/$1"
        shift
    done
}

___x_cmd_xrc_cat(){
    if [ $# -gt 0 ]; then
        ___x_cmd_xrc_cat___ "$@"
        return
    fi
    ___x_cmd_cmds_cat "$___X_CMD_ROOT_MOD/xrc/lib/help/cat.txt" >&2
    return 1
}
